{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react", "validator"
    ],
    "rules": {
      "no-underscore-dangle": ["error", {"allow": ["_id"]}]
    }
}
